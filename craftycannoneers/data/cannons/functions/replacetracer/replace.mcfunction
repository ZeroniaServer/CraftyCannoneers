#> Failsafe
execute unless entity @s[nbt={Inventory:[{id:"minecraft:diamond_hoe",tag:{CustomModelData:91}}]}] unless entity @s[nbt={Inventory:[{id:"minecraft:diamond_hoe",tag:{CustomModelData:60}}]}] run tag @s add GiveNewTracer
execute if entity @s[nbt={Inventory:[{id:"minecraft:diamond_hoe",tag:{CustomModelData:91}}]}] run tag @s add GiveNewTracer
execute if entity @s[tag=GiveNewTracer] run clear @s diamond_hoe{CustomModelData:91}
execute if entity @s[tag=GiveNewTracer] run clear @s diamond_hoe{CustomModelData:60}
execute if entity @s[tag=GiveNewTracer,tag=fullinv] run tag @s add NeedsTracer
execute if entity @s[tag=GiveNewTracer,tag=!fullinv] run loot give @s loot cannons:tracer
tag @s[tag=GiveNewTracer] remove GiveNewTracer